{
  "name": "@medisys/authorization",
  "version": "0.1.40",
  "description": "Medisys UI Access Control Package",
  "keywords": [
    "medisys"
  ],
  "author": "Chris Lu Yi <chris_lu@medinno.onmicrosoft.com>",
  "homepage": "https://github.com/medisysinnovation/medisys#readme",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": "dist/index.modern.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "directories": {
    "src": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medisysinnovation/medisys.git"
  },
  "scripts": {
    "tsc": "tsc --noEmit",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
